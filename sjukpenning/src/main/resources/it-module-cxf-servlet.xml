<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:http="http://cxf.apache.org/transports/http/configuration"
    xmlns:cxf="http://cxf.apache.org/core" xmlns:jaxws="http://cxf.apache.org/jaxws"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://cxf.apache.org/core
        http://cxf.apache.org/schemas/core.xsd
        http://cxf.apache.org/transports/http/configuration
        http://cxf.apache.org/schemas/configuration/http-conf.xsd
        http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

  <cxf:bus>
    <cxf:features>
      <cxf:logging/>
    </cxf:features>
  </cxf:bus>

  <!-- ws endpoints --><!--jaxws:endpoint address="/get-sjukpenning/v1.0"
      implementor="se.inera.certificate.modules.sjukpenning.integration.GGetTSBasResponderImpl">
    <jaxws:outFaultInterceptors>
      <bean
          class="se.inera.certificate.interceptor.SoapFaultToSoapResponseTransformerInterceptor">
        <constructor-arg
            value="transform/se-intygstjanster-ts-services/get-ts-bas-transform.xslt"/>
      </bean>
    </jaxws:outFaultInterceptors>
  </jaxws:endpoint -->

  <bean class="se.inera.certificate.modules.sjukpenning.integration.RegisterSjukpenningResponderImpl" />

  <jaxws:endpoint address="/register-sjukpenning/v1.0"
      implementor="se.inera.certificate.modules.sjukpenning.integration.RegisterSjukpenningResponderImpl">
    <jaxws:outFaultInterceptors>
      <bean class="se.inera.certificate.interceptor.SoapFaultToSoapResponseTransformerInterceptor">
        <constructor-arg value="transform/register-sjukpenning-transform.xslt"/>
      </bean>
    </jaxws:outFaultInterceptors>
    <jaxws:properties>
      <entry key="schema-validation-enabled" value="true"/>
    </jaxws:properties>
    <jaxws:schemaLocations>
      <jaxws:schemaLocation>classpath:/core_components/se_intygstjanster_services_fk_1.0.xsd</jaxws:schemaLocation>
      <jaxws:schemaLocation>classpath:/interactions/RegisterSjukpenningInteraction/RegisterSjukpenningResponder_1.0.xsd</jaxws:schemaLocation>
    </jaxws:schemaLocations>
  </jaxws:endpoint>

</beans>
