<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="transportToExternalConverter"
    class="se.inera.certificate.modules.rli.model.converters.TransportToExternalConverter" />

  <bean id="externalToTransportConverter"
    class="se.inera.certificate.modules.rli.model.converters.ExternalToTransportConverter" />

  <bean id="externalValidator"
    class="se.inera.certificate.modules.rli.validator.ExternalValidator" />

  <bean id="externalToInternalConverter"
    class="se.inera.certificate.modules.rli.model.converters.ExternalToInternalConverter" />

  <bean id="rekommendationPopulator"
    class="se.inera.certificate.modules.rli.model.converters.RekommendationPopulator" />

  <bean id="undersokningPopulator"
    class="se.inera.certificate.modules.rli.model.converters.UndersokningPopulator" />

  <bean id="pdfGenerator" class="se.inera.certificate.modules.rli.pdf.PdfGenerator" />

  <bean id="editModelFactory"
    class="se.inera.certificate.modules.rli.model.factory.EditModelFactory" />

</beans>